
            <html><head><meta charset="UTF-8"></head>
            <body>
            <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
            <script type="text/javascript">
            google.charts.load('current', {packages: ['corechart'], 'language': 'pt'});
            google.charts.setOnLoadCallback(drawChart);

            function drawChart(dados, tipo, container) {
              var data = new google.visualization.DataTable();
              data.addColumn('string', 'Mês');
              data.addColumn('number', new Date().getFullYear().toString());
              data.addColumn('number', "Máx");
              data.addColumn('number', "Méd");
              data.addColumn('number', "Mín");

              data.addRows([['Jan', 282.0, 544, 195.0, 7], ['Fev', 107.0, 567, 133.0, 8], ['Mar', 81.0, 148, 48.0, 6], ['Abr', 200.0, 257, 73.0, 8], ['Mai', 83.0, 370, 152.0, 1], ['Jun', 0.0, 548, 256.0, 2], ['Jul', 0.0, 2569, 805.0, 5], ['Ago', 0.0, 7368, 3117.0, 79], ['Set', 0.0, 6152, 3755.0, 77], ['Out', 0.0, 1910, 1020.0, 64], ['Nov', 0.0, 1512, 436.0, 25], ['Dez', 0.0, 398, 169.0, 4]]);

              var options = {
                title: '',
                vAxis: { title: 'N° Focos' },
                hAxis: { title: 'Mês' },
                seriesType: 'bars',
                focusTarget: 'category',
                bar: { groupWidth: 10},
                pointSize: 3,
                series: { 1: { type: 'line' }, 2: { type: 'line', visibleInLegend: true }, 3: { type: 'line', visibleInLegend: true } },
                width: 900,
                height: 450
              };
              var chart = new google.visualization.ComboChart(document.getElementById('comparativo_historico_paises'));
              chart.draw(data, options);
              }
            </script>
            <div>
