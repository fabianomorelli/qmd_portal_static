
<html>
<head><meta charset="UTF-8"><meta charset="UTF-8"><head><meta charset="UTF-8"><body><h3 style="text-align:center;">Quantidade de focos por países no mês de 01/Set até 07/Set</h3>
  <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>


<script type="text/javascript">
google.charts.load('current', {packages: ['corechart']});
google.charts.setOnLoadCallback(drawChart);

function drawChart(dados, tipo, container) {
  
  var data = new google.visualization.DataTable();
  data.addColumn('string', 'Element');
  data.addColumn('number', 'Focos');
  data.addRows( [['Brasil', 16824.0], ['Bolivia', 1801.0], ['Argentina', 943.0], ['Paraguai', 751.0], ['Perú', 654.0], ['Colombia', 49.0], ['Chile', 22.0], ['Venezuela', 22.0], ['Equador', 12.0], ['Uruguai', 8.0], ['Guyana', 4.0], ['Guyana Francesa', 3.0], ['Suriname', 3.0]]);

  var options = {
    chart: {
      title: 'Focos de incêndio',
      subtitle: 'Focos detectados pelo Satélite de Referência',      
    }
  };

  var chart = new google.visualization.ColumnChart(document.getElementById("chart_paises"));
  chart.draw(data, options);
}
</script>
 <div id="chart_paises" style="width: 100%; height: 100%;"></div>
</body>
</html>
