
<html>
<head><meta charset="UTF-8"><head><body><h3 style="text-align:center;">Quantidade de focos por estados do Brasil no ano atual de 01/Jan até 19/Abr</h3>
  <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>


<script type="text/javascript">
google.charts.load('current', {packages: ['corechart']});
google.charts.setOnLoadCallback(drawChart);

function drawChart(dados, tipo, container) {
  
  var data = new google.visualization.DataTable();
  data.addColumn('string', 'Element');
  data.addColumn('number', 'Focos');
  data.addRows( [['MT', 4220], ['MS', 2046], ['RR', 1648], ['RS', 1060], ['PR', 746], ['TO', 710], ['SP', 678], ['SC', 678], ['BA', 581], ['GO', 486], ['MA', 451], ['MG', 445], ['PA', 433], ['AM', 362], ['PI', 191], ['RO', 176], ['PE', 168], ['CE', 106], ['AL', 84], ['PB', 83], ['RJ', 54], ['SE', 54], ['RN', 47], ['ES', 43], ['AC', 40], ['DF', 12], ['AP', 4]]);

  var options = {
    chart: {
      title: 'Focos de incêndio',
      subtitle: 'Focos detectados pelo Satélite de Referência',      
    }
  };

  var chart = new google.visualization.ColumnChart(document.getElementById("chart_paises"));
  chart.draw(data, options);
}
</script>
 <div id="chart_paises" style="width: 100%; height: 100%;"></div>
