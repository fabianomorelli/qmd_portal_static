
<html>
<head><meta charset="UTF-8"><head><body><h3 style="text-align:center;">Quantidade de focos por municípios do Brasil nas últimas 48h</h3>
  <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>


<script type="text/javascript">
google.charts.load('current', {packages: ['corechart']});
google.charts.setOnLoadCallback(drawChart);

function drawChart(dados, tipo, container) {
  
  var data = new google.visualization.DataTable();
  data.addColumn('string', 'Element');
  data.addColumn('number', 'Focos');
  data.addRows( [['PORTO MURTINHO (MATO GROSSO DO SUL)', 11], ['TABAPORÃ (MATO GROSSO)', 9], ['CORUMBÁ (MATO GROSSO DO SUL)', 8], ['JUARA (MATO GROSSO)', 6], ['ENCRUZILHADA DO SUL (RIO GRANDE DO SUL)', 5], ['BALSAS (MARANHÃO)', 4], ['BELA VISTA (MATO GROSSO DO SUL)', 4], ['CERRO AZUL (PARANÁ)', 3], ['BRASNORTE (MATO GROSSO)', 3], ['BANDEIRANTES (PARANÁ)', 3]]);

  var options = {
    chart: {
      title: 'Focos de incêndio',
      subtitle: 'Focos detectados pelo Satélite de Referência',      
    }
  };

  var chart = new google.visualization.PieChart(document.getElementById("chart_paises"));
  chart.draw(data, options);
}
</script>
 <div id="chart_paises" style="width: 100%; height: 100%;"></div>
